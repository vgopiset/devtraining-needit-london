        <script><![CDATA[var currentTime = new GlideDateTime();

var records = new GlideRecord('x_58872_needit_needit_task');
records.addQuery('due_date','<=',currentTime);
records.addQuery('state','!=',3);

records.query();

while(records.next()) {
	gs.info("Records:" + records.number);
	gs.eventQueue('x_58872_needit.overdueNeedItTask',records,records.number , gs.getUserName(),'needittask_queue');
	gs.info('EventRaised');
}

]]></script>
